{
  "name": "radiant-player",
  "version": "2.0.0",
  "license": "MIT",
  "main": "dist/backend",
  "repository": {
    "type": "git",
    "url": "https://github.com/radiant-player/radiant-player-electron.git"
  },
  "scripts": {
    "build:backend": "babel-node ./node_modules/.bin/webpack --config support/config/webpack.backend.js --colors",
    "build:app": "babel-node ./node_modules/.bin/webpack --config support/config/webpack.app.js --colors",
    "build": "npm run build:app && npm run build:backend",
    "hot:backend": "cross-env HOT=1 npm run watch:backend",
    "hot:server": "cross-env HOT=1 babel-node support/hot-server.js",
    "hot:app": "cross-env HOT=1 npm run watch:app",
    "hot": "concurrently --kill-otherconcurrently --kill-others \"npm run hot:backend\" \"npm run hot:server\" \"npm run hot:app\"",
    "lint": "eslint .",
    "prod:backend": "cross-env NODE_ENV=production npm run build:backend",
    "prod:app": "cross-env NODE_ENV=production npm run build:app",
    "prod": "cross-env NODE_ENV=production npm run build",
    "start": "cross-env NODE_ENV=production electron ./",
    "start:hot": "cross-env HOT=1 electron ./",
    "test": "npm run lint",
    "watch:backend": "npm run build:backend -- --watch",
    "watch:app": "npm run build:app -- --watch",
    "watch": "concurrently --kill-others \"npm run watch:backend\" \"npm run watch:app\""
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "concurrently": "^2.0.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "electron-prebuilt": "^0.37.5",
    "electron-rebuild": "^1.1.3",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "postcss": "^5.0.19",
    "postcss-import": "^8.1.0",
    "postcss-loader": "^0.8.2",
    "postcss-scss": "^0.1.7",
    "precss": "^1.4.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.3",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-filter-actions": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.9",
    "style-loader": "^0.13.1",
    "text-loader": "0.0.1",
    "watch": "^0.17.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.0.0",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "dependencies": {
    "deep-diff": "^0.3.3",
    "electron-debug": "^0.6.0",
    "electron-positioner": "^2.0.3",
    "electron-window-state": "^2.0.0",
    "gmusic.js": "^4.2.0",
    "invariant": "^2.2.1",
    "lodash.throttle": "^4.0.1",
    "nan": "^2.2.1",
    "node-notifier": "^4.5.0",
    "node-uuid": "^1.4.7",
    "rc-slider": "^3.6.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-electron-webview": "^1.0.7",
    "react-redux": "^4.4.2",
    "redux": "^3.4.0",
    "redux-actions": "^0.9.1",
    "redux-electron-store": "^0.3.6",
    "redux-logger": "^2.6.1",
    "redux-promise": "^0.5.3",
    "redux-storage": "^4.0.0",
    "redux-storage-engine-localstorage": "^1.0.2",
    "redux-thunk": "^2.0.1"
  }
}
